{
  "name": "@trademe/ensure",
  "version": "0.0.0-PLACEHOLDER",
  "license": "MIT",
  "description": "Utility decorators for Angular applications",
  "scripts": {
    "build": "ng build && npm run lint && npm run cover",
    "cover": "node node_modules/istanbul/lib/cli.js cover --config .istanbulrc.json node_modules/mocha/bin/_mocha -- --opts ./.mocharc \"projects/ensure/src/**/*.spec.ts\"",
    "test": "mocha --opts ./.mocharc \"projects/ensure/src/**/*.spec.ts\"",
    "test:debug": "mocha --opts ./.mocharc \"projects/ensure/src/**/*.spec.ts\" --inspect-brk",
    "lint": "ng lint"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.1",
    "@angular-devkit/build-ng-packagr": "^0.12.1",
    "@angular/cli": "^7.2.1",
    "@angular/common": "^7.2.1",
    "@angular/compiler": "^7.2.1",
    "@angular/compiler-cli": "^7.2.1",
    "@angular/core": "^11.0.5",
    "@angular/language-service": "^7.2.1",
    "@angular/platform-browser": "^7.2.1",
    "@angular/platform-browser-dynamic": "^7.2.1",
    "@types/chai": "^3.5.2",
    "@types/core-js": "^0.9.41",
    "@types/mocha": "^2.2.41",
    "@types/node": "~8.9.4",
    "chai": "^3.5.0",
    "codelyzer": "^4.5.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "ng-packagr": "^4.4.5",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.3.3",
    "ts-node": "~5.0.1",
    "tsickle": "^0.34.0",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "^3.1.6",
    "zone.js": "^0.8.26"
  }
}
