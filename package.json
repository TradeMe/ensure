{
  "name": "@trademe/ensure",
  "version": "0.0.0-PLACEHOLDER",
  "license": "MIT",
  "description": "Utility decorators for Angular applications",
  "scripts": {
    "build": "ng build && npm run lint && npm run cover",
    "cover": "node node_modules/istanbul/lib/cli.js cover --config .istanbulrc.json node_modules/mocha/bin/_mocha -- --opts ./.mocharc \"projects/ensure/src/**/*.spec.ts\"",
    "test": "mocha --opts ./.mocharc \"projects/ensure/src/**/*.spec.ts\"",
    "test:debug": "mocha --opts ./.mocharc \"projects/ensure/src/**/*.spec.ts\" --inspect-brk",
    "lint": "ng lint"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.18",
    "@angular/cli": "^12.2.18",
    "@angular/common": "^12.2.17",
    "@angular/compiler": "^12.2.17",
    "@angular/compiler-cli": "^12.2.17",
    "@angular/core": "^12.2.17",
    "@angular/language-service": "^12.2.17",
    "@angular/platform-browser": "^12.2.17",
    "@angular/platform-browser-dynamic": "^12.2.17",
    "@types/chai": "^3.5.2",
    "@types/core-js": "^0.9.41",
    "@types/mocha": "^2.2.41",
    "@types/node": "^12.11.1",
    "chai": "^3.5.0",
    "codelyzer": "^6.0.0",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.3.3",
    "ts-node": "~5.0.1",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "4.3.5",
    "zone.js": "~0.11.4",
    "@angular-devkit/schematics": "12.2.18"
  }
}
