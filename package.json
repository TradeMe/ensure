{
  "name": "@trademe/ensure",
  "version": "0.1.0-beta.5",
  "description": "Utility decorators for Angular applications",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "npm run clean && npm run compile && npm run cover",
    "clean": "rimraf ./compiled && rimraf ./dist",
    "compile": "ngc -p ./tsconfig.json && tsc -p ./tsconfig.json",
    "cover": "ts-node node_modules/istanbul/lib/cli.js cover --config .istanbulrc.json node_modules/mocha/bin/_mocha -- --opts ./.mocharc \"src/**/*.spec.ts\"",
    "test": "mocha --opts ./.mocharc \"src/**/*.spec.ts\""
  },
  "author": "Craig Spence <craig@trademe.co.nz>",
  "license": "MIT",
  "peerDependencies": {
    "@angular/core": "^4.0.0"
  },
  "dependencies": {
    "dedent": "0.7.0"
  },
  "devDependencies": {
    "@angular/compiler": "^4.1.0",
    "@angular/compiler-cli": "^4.1.0",
    "@angular/core": "^4.1.0",
    "@types/chai": "^3.5.2",
    "@types/core-js": "^0.9.41",
    "@types/mocha": "^2.2.41",
    "@types/node": "^7.0.14",
    "@types/reflect-metadata": "^0.0.5",
    "@types/sinon": "^2.1.3",
    "chai": "^3.5.0",
    "core-js": "^2.4.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^3.3.0",
    "reflect-metadata": "^0.1.10",
    "rimraf": "^2.6.1",
    "rxjs": "^5.3.0",
    "sinon": "^2.1.0",
    "ts-node": "^3.0.2",
    "typescript": "^2.2.2"
  }
}
