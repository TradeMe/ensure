{
  "name": "@trademe/ensure",
  "version": "0.0.0-PLACEHOLDER",
  "license": "MIT",
  "description": "Utility decorators for Angular applications",
  "scripts": {
    "build": "ng build && npm run lint && npm run cover",
    "cover": "node node_modules/istanbul/lib/cli.js cover --config .istanbulrc.json node_modules/mocha/bin/_mocha -- --opts ./.mocharc \"projects/ensure/src/**/*.spec.ts\"",
    "test": "mocha --opts ./.mocharc \"projects/ensure/src/**/*.spec.ts\"",
    "test:debug": "mocha --opts ./.mocharc \"projects/ensure/src/**/*.spec.ts\" --inspect-brk",
    "lint": "ng lint"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.8.1",
    "@angular-devkit/build-ng-packagr": "~0.11.0",
    "@angular/cli": "~6.2.1",
    "@angular/common": "^6.1.7",
    "@angular/compiler": "^6.1.7",
    "@angular/compiler-cli": "^6.1.7",
    "@angular/core": "^6.1.7",
    "@angular/language-service": "^6.1.7",
    "@angular/platform-browser": "^6.1.7",
    "@angular/platform-browser-dynamic": "^6.1.7",
    "@types/chai": "^3.5.2",
    "@types/core-js": "^0.9.41",
    "@types/mocha": "^2.2.41",
    "@types/node": "~8.9.4",
    "chai": "^3.5.0",
    "codelyzer": "~4.2.1",
    "istanbul": "^1.1.0-alpha.1",
    "mocha": "^5.2.0",
    "ng-packagr": "^3.0.0-rc.2",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.1.0",
    "ts-node": "~5.0.1",
    "tsickle": ">=0.25.5",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2",
    "zone.js": "^0.8.26"
  }
}
